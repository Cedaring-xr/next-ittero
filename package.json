    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "sst": "^2.49.6",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  }
