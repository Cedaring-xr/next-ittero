{
  "name": "next-ittero",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:e2e": "playwright test -c playwright.config.ts",
    "test:component": "playwright test -c playwright-ct.config.ts",
    "test:smoke": "playwright test -c playwright.config.ts --grep @smoke",
    "test:regression": "playwright test -c playwright.config.ts --grep @regression",
    "test:all": "npm run test:component && npm run test:e2e",
    "deploy:staging": "npx sst@2 deploy --stage staging --no-interactive",
    "deploy:prod": "npx sst@2 deploy --stage prod --no-interactive"
  },
  "dependencies": {
    "@aws-amplify/adapter-nextjs": "^1.5.8",
