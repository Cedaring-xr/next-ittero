    "@eslint/config-array": "^0.21.1",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query": "^4.42.0",
    "aws-amplify": "^6.14.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "lru-cache": "^10.2.0",
    "next": "^14.2.33",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.65.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.6.0",
